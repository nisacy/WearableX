source 'https://github.com/CocoaPods/Specs.git'
platform :ios, "7.0"
inhibit_all_warnings!



target :Wearable  do


#A delightful iOS and OS X networking framework.
pod 'AFNetworking', '~> 3.1.0'
#- Homepage: https://github.com/AFNetworking/AFNetworking
#- Source:   https://github.com/AFNetworking/AFNetworking.git

#A Very High Performance Objective-C JSON Library.
pod 'JSONKit', '~> 1.5pre'
#- Homepage: https://github.com/johnezang/JSONKit
#- Source:   https://github.com/johnezang/JSONKit.git

#Asynchronous image downloader with cache support with an UIImageView category.
pod 'SDWebImage', '~> 3.8.1'
#- Homepage: https://github.com/rs/SDWebImage
#- Source:   https://github.com/rs/SDWebImage.git

#A clean and lightweight progress HUD for your iOS and tvOS app.
pod 'SVProgressHUD', '~> 2.0.3'
#- Homepage: https://github.com/SVProgressHUD/SVProgressHUD
#- Source:   https://github.com/SVProgressHUD/SVProgressHUD.git

#An easy way to use pull-to-refresh
pod 'MJRefresh', '~> 3.1.10'
#- Homepage: https://github.com/CoderMJLee/MJRefresh
#- Source:   https://github.com/CoderMJLee/MJRefresh.git

#A fast and convenient conversion between JSON and model
pod 'MJExtension', '~> 3.0.11'
#- Homepage: https://github.com/CoderMJLee/MJExtension
#- Source:   https://github.com/CoderMJLee/MJExtension.git

#A drop-in UITableView/UICollectionView superclass category for showing emptydatasets whenever the view has no content to display.
pod 'DZNEmptyDataSet', '~> 1.8.1'
#- Homepage: https://github.com/dzenbot/DZNEmptyDataSet
#- Source:   https://github.com/dzenbot/DZNEmptyDataSet.git

#无IDFA版SDK（请根据需要选择其中一个）
pod 'UMengAnalytics-NO-IDFA'


#Harness the power of Auto Layout NSLayoutConstraints with a simplified, chainable and expressive syntax.
pod 'Masonry', '~> 1.0.1'
#- Homepage: https://github.com/cloudkite/Masonry
#- Source:   https://github.com/cloudkite/Masonry.git

#emplate auto layout cell for automatically UITableViewCell height calculate,cache and precache
pod 'UITableView+FDTemplateLayoutCell', '~> 1.5.beta'
#- Homepage: https://github.com/forkingdog/UITableView-FDTemplateLayoutCell
#- Source: https://github.com/forkingdog/UITableView-FDTemplateLayoutCell.git


#Simple Cocoa wrapper for the keychain.
#pod 'SSKeychain', '~> 1.4.0'
#- Homepage: https://github.com/soffes/sskeychain
#- Source:   https://github.com/soffes/sskeychain.git


#An iOS activity indicator view.
#pod 'MBProgressHUD', '~> 0.9.2'
#- Homepage: http://www.bukovinski.com
#- Source:   https://github.com/matej/MBProgressHUD.git

pod 'NJKWebViewProgress'

pod 'UIFloatLabelTextField'

pod 'SAMKeychain', :git => 'https://github.com/soffes/SAMKeychain'


pod 'MMDrawerController', '~> 0.5.7'

pod 'BabyBluetooth','~> 0.7.0'

end




